      "source": [
        "# Error Estándar Residual (RSE)\n",
        "n = len(y)\n",
        "p = X.shape[1] - 1  # número de predictores\n",
        "RSS = np.sum(residuals ** 2)\n",
        "RSE = np.sqrt(RSS / (n - p - 1))\n",
        "print(f\"Error Estándar Residual (RSE): {RSE:.4f}\")"
      ],
      "metadata": {
        "colab": {
