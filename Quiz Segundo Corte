        "# Crear datos sintéticos\n",
        "np.random.seed(42)\n",
        "X1 = np.random.rand(100)\n",
        "X2 = X1 + np.random.normal(0, 0.1, 100)\n",
        "X3 = np.random.rand(100)\n",
        "y =(5 * X1 + 2 * X3 + np.random.normal(0, 0.1, 100))*10\n",
        "\n",
        "df = pd.DataFrame({'X1': X1, 'X2': X2, 'X3': X3})\n",
        "df"
